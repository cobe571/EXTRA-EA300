<?xml version="1.0" encoding="UTF-8"?>

<!-- Manifold et Fuel Flow Extra 300L
Ensemble à revoir
-->
<PropertyList> 

<!-- Nettoyage des noms d'objets -->
  <path>man.ac</path>

  <animation>
    <object-name>contour</object-name>
    <object-name>Fond</object-name>
    <object-name>NeedleM</object-name>
    <object-name>NeedleF</object-name>

    <object-name>MANvitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>HDGvitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
    <object-name>MANvitre</object-name>
  </animation>
 
<!-- éclairage de l'instru 
  <animation>
    <type>material</type>
    <object-name>Fond</object-name>
    <object-name>NeedleM</object-name>
    <object-name>NeedleF</object-name>a
    <emission>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
    </emission>
  </animation> -->


  <animation>
    <type>rotate</type>
    <object-name>NeedleM</object-name>
    <property>engines/engine/mp-osi</property>
    <interpolation>
      <entry><ind>  0 </ind><dep>   0 </dep></entry>
      <entry><ind> 10 </ind><dep>   5 </dep></entry>
      <entry><ind> 15 </ind><dep>  38 </dep></entry>
      <entry><ind> 20 </ind><dep>  69 </dep></entry>
      <entry><ind> 25 </ind><dep> 100 </dep></entry>
      <entry><ind> 30 </ind><dep> 130 </dep></entry>
      <entry><ind> 35 </ind><dep> 163 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>NeedleF</object-name>
    <property>engines/engine/fuel-flow-gph</property>
    <interpolation>
      <entry><ind>  0 </ind><dep>   0 </dep></entry>
      <entry><ind> 10 </ind><dep>  18 </dep></entry>
      <entry><ind> 15 </ind><dep>  40 </dep></entry>
      <entry><ind> 20 </ind><dep>  62 </dep></entry>
      <entry><ind> 25 </ind><dep>  93 </dep></entry>
      <entry><ind> 30 </ind><dep> 135 </dep></entry>
      <entry><ind> 35 </ind><dep> 164 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>

