      </less-than>
    </condition>
    <property>/autopilot/KAP140/settings/target-pressure-rate</property>
    <factor>-5.8</factor>
    <step>0.017241379</step>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>vs-digit5-minus</object-name>
