    <object-name>apr-btn</object-name>
    <object-name>rev-btn</object-name>
    <object-name>alt-btn</object-name>
    <object-name>dn-btn</object-name>
    <object-name>up-btn</object-name>
    <object-name>arm-btn</object-name>
