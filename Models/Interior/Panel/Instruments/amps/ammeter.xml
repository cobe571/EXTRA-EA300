<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>ammeter.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>visses</object-name>
    <object-name>Ammeter</object-name>
    <object-name>AMPgauge</object-name>
    <object-name>AMPneedle</object-name>

    <object-name>vitre</object-name>
  </animation>

  <!-- Rembrandt -->
  <effect>
    <inherits-from>Aircraft/EA300/Models/Effects/Glass/glass</inherits-from>
    <object-name>vitre</object-name>
  </effect>
  
  <animation>
    <type>material</type>
    <object-name>AMPgauge</object-name>
    <object-name>AMPneedle</object-name>
    <emission>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>AMPneedle</object-name>
    <property>/systems/electrical/amps</property>
    <interpolation>
      <entry><ind> -60 </ind><dep> -60 </dep></entry>
      <entry><ind>   0 </ind><dep>   0 </dep></entry>
      <entry><ind>  60 </ind><dep>  60 </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.00000 </x-m>
      <y-m>  0.00000 </y-m>
      <z-m> -0.00568 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>