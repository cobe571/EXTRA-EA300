<?xml version="1.0" encoding="UTF-8"?>

<!-- Pilot-Gmeter -->

<PropertyList>

  <path>gmeter.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>visses</object-name>
    <object-name>face</object-name>
    <object-name>g-Needle1</object-name>
    <object-name>g-Needle2</object-name>
    <object-name>g-Needle3</object-name>

    <object-name>vitre</object-name>
  </animation>

  <effect>
    <inherits-from>Aircraft/EXTRA-EA300/Models/Effects/Glass/glass</inherits-from>
    <object-name>vitre</object-name>
  </effect>
  
  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>g-Needle1</object-name>
    <object-name>g-Needle2</object-name>
    <object-name>g-Needle3</object-name>
    <emission>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
    </emission>
  </animation>    

  <animation>
    <type>rotate</type>
    <object-name>g-Needle1</object-name>
    <property>accelerations/pilot-g</property>
    <interpolation>
     <entry><ind> -4 </ind><dep> -200.0 </dep></entry>
     <entry><ind> -2 </ind><dep> -156.0 </dep></entry>
     <entry><ind> -1 </ind><dep> -134.0 </dep></entry>
     <entry><ind>  0 </ind><dep> -112.0 </dep></entry>
     <entry><ind>  1 </ind><dep>  -90.0 </dep></entry>
     <entry><ind>  2 </ind><dep>  -68.0 </dep></entry>
     <entry><ind>  3 </ind><dep>  -46.0 </dep></entry>
     <entry><ind>  4 </ind><dep>  -24.0 </dep></entry>
     <entry><ind>  5 </ind><dep>   -2.0 </dep></entry>
     <entry><ind>  6 </ind><dep>   20.0 </dep></entry>
     <entry><ind>  8 </ind><dep>   64.0 </dep></entry>
     <entry><ind>  9 </ind><dep>   86.0 </dep></entry>
     <entry><ind> 10 </ind><dep>  107.5 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>g-Needle2</object-name>
    <property>accelerations/g-min</property>
    <interpolation>
      <entry><ind> -4 </ind><dep> -200 </dep></entry>
      <entry><ind> -2 </ind><dep> -156 </dep></entry>
      <entry><ind> -1 </ind><dep> -134 </dep></entry>
      <entry><ind>  0 </ind><dep> -112 </dep></entry>
      <entry><ind>  1 </ind><dep>  -90 </dep></entry>
    </interpolation>
    <axis >
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>g-Needle3</object-name>
    <property>accelerations/g-max</property>
    <interpolation>
      <entry><ind>  1 </ind><dep> -90.0 </dep></entry>
      <entry><ind>  2 </ind><dep> -68.0 </dep></entry>
      <entry><ind>  3 </ind><dep> -46.0 </dep></entry>
      <entry><ind>  4 </ind><dep> -24.0 </dep></entry>
      <entry><ind>  5 </ind><dep>  -2.0 </dep></entry>
      <entry><ind>  6 </ind><dep>  20.0 </dep></entry>
      <entry><ind>  8 </ind><dep>  64.0 </dep></entry>
      <entry><ind>  9 </ind><dep>  86.0 </dep></entry>
      <entry><ind> 10 </ind><dep> 107.5 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>